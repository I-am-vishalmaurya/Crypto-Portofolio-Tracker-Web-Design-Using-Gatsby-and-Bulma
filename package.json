  },
  "dependencies": {
    "bulma": "^0.9.4",
    "gatsby": "^4.24.4",
    "node-sass": "^7.0.3",
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.45",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "typescript": "^4.8.3"
  }
}
