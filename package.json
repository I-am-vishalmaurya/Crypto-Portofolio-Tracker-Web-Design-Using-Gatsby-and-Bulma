{
  "name": "crypto-tracker-designs",
  "version": "1.0.0",
  "private": true,
  "description": "crypto-tracker-designs",
  "author": "pythonicnerd",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "css-watch": "npm run css-build -- --watch",
    "develop": "gatsby develop",
    "start": "gatsby develop && npm run css-watch",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "typecheck": "tsc --noEmit",
